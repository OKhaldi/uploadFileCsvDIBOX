    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "22.1.0",
    "msw": "^1.3.2",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vite": "4.5.0",
    "vite-plugin-eslint": "^1.8.1",
