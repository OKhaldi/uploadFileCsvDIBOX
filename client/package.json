{
  "name": "altima-front-echo",
  "version": "0.0.1",
  "description": "Front End Echo",
  "type": "module",
  "engines": {
    "node": ">=16.20.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
